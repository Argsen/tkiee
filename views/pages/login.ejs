                        success: function(response) {
                            if (response.error) {
                                alert(response.error);
                            } else {
                                window.location.href = 'dashboard';
                            }
                        }
                    });	                 
                },
                Cancel: function() {
                    $("#dialog-zipUrl").dialog("close");
					$("#zipUrl").empty();
                }
            }			
		});
		
