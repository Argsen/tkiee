									if (response.error) {
										alert(response.error);
									} else {
										$("#dialog-form").dialog("close");
										alert("New User Created!");
										$.ajax({
											type: "POST",
											url: "User/login",
											dataType: "json",
											cache: false,
											data: {"email": email.val(), "password": password.val()},
											success: function(response) {
												console.log(response);
												//var res = JSON.parse(response);
												userLogin(response);
											}
										});										
									}
								}
							});					
						}
					}
                },
                Cancel: function() {
                    $("#dialog-form").dialog("close");
                }
            }
        });
		
