                                                    var blob = new Blob([statisticExport], {type: "text/csv;charset=utf-8;",});
                                                    saveAs(blob, "Statistics.txt");  													
                                                }   
                                            });			*/								
                                        });												
                                    } else {
                                        a.text("Download Evaluation File.");
